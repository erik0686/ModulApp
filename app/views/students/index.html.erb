<%= render "layouts/navbar" %>
<div class="student-list">
	<% @students.each do |student| %>
    <span>-----------------------</span>
    <h5><%= link_to student.name, student_path(student) %></h5>
		<li><%= student.matricula %></li>
		<li><%= student.program_type %></li>
		<li><%= student.semester.ordinalize %> semester</li>
	<% end %>
</div>
